<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.YH.travelaccount.travel.mapper.TravelMapper">

    <insert id="insert" parameterType="com.YH.travelaccount.travel.domain.Travel"
            useGeneratedKeys="true" keyProperty="id">

        INSERT INTO travel (
            title,
            start_date,
            end_date,
            created_by,
            created_at
        ) VALUES (
                     #{title},
                     #{startDate},
                     #{endDate},
                     #{createdBy},
                     NOW()
                 )

    </insert>

</mapper>
